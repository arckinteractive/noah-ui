{
  "name": "@arckinteractive/noah-ui",
  "version": "0.1.0",
  "license": "MIT",
  "main": "src/main.js",
  "scripts": {
    "lint": "vue-cli-service lint",
    "test:unit": "nyc vue-cli-service test:unit",
    "dev": "yarn concurrently \"yarn components:parse\" \"vuepress dev docs\"",
    "build": "babel-node ./.scripts/components/parse.js && cross-env BUILD_BASE=/noah-ui/ vuepress build docs && vue-cli-service build --target lib ./src/main.js",
    "build:local": "babel-node ./.scripts/components/parse.js && vuepress build docs",
    "components:parse": "nodemon --watch src -e vue --exec babel-node ./.scripts/components/parse.js",
    "deploy": "./.scripts/deploy.sh"
  },
  "dependencies": {
    "core-js": "^2.6.9",
    "lodash.debounce": "^4.0.8",
    "markdown-it": "^8.4.2",
    "popper.js": "^1.15.0",
    "raw-loader": "^2.0.0",
    "svg-country-flags": "^1.2.6",
    "tiny-slider": "^2.9.2",
    "vue": "^2.6.10",
    "vue-click-outside": "^1.0.7",
    "vue-codemirror": "^4.0.6",
    "vue-meta": "^1.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-unit-mocha": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "@vuedoc/parser": "^2.0.0-beta.4",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.4",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-es6": "^1.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-vue": "^5.2.2",
    "file-api": "^0.10.4",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "node-sass": "^4.12.0",
    "nyc": "^14.1.1",
    "postcss": "^7.0.16",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "sass-loader": "^7.1.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^0.14.11",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "include": [
      "src/**/*.js",
      "src/**/*.vue"
    ],
    "extension": [
      ".js",
      ".vue"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "all": true,
    "sourceMap": true,
    "report-dir": "./coverage"
  }
}
